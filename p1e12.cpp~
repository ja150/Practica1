#include <iostream>
#include <cstring>

using namespace std;

void copiar_cadena(char *p, char *q);

int main(){
	char cadena1[] = {'R','U', 'B', 'E', 'N','\0'};
	char cadena2[20];
	char *p=cadena2, *q=cadena1;
	copiar_cadena(p, q);
	cout << p << endl;
}

void copiar_cadena(char *p, char *q){
	while(*q!='\0'){	
		*p=*q;
		p++;
		q++;
	}
	*p = '\0';	
}
